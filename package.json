        "prettier": "echo \"â³\" && onchange './_scss/**/*.scss' './_libs/**/*.css' './_js/**/*.js' './_tests/_js/**/*.js' './_html/**/*.html' './_php/**/*.php' -- prettier --write {{changed}} && echo \"âœ…\"",
        "update-changelog": "echo \"â³\" && auto-changelog --commit-limit false --package && echo \"âœ…\"",
        "                                                                                                                                                                                                          ": "",
        "ðŸ”½ pipelines ðŸ”½": "",
        "prod": "cross-env NODE_ENV=production npm-run-all --print-label --silent --sequential css:slim js:slim",
        "dev": "cross-env NODE_ENV=development npm-run-all --print-label --silent --parallel css:slim:watch js:slim:watch browsersync",
        "prod:full": "cross-env NODE_ENV=production npm-run-all --print-label --silent --sequential copy css js html delete:files",
        "dev:full": "cross-env NODE_ENV=development npm-run-all --print-label --silent --parallel copy:watch css:watch js:watch js:tests:watch html:watch browsersync"
