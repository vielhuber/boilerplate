        "                                                                                                                                                                                                        ": "",
        "ðŸ”½ browsersync ðŸ”½": "",
        "browsersync": "from-env browser-sync start --no-open --log-level silent --proxy %DOMAIN",
        "browsersync:reload": "browser-sync reload",
        "browsersync:watch": "from-env browser-sync start --no-open --log-level silent --reload-debounce 3000 --proxy %DOMAIN --port 3030 --files \"./$npm_package_config_build_folder/*.html, ./$npm_package_config_build_folder/*.css, ./$npm_package_config_build_folder/*.js\"",
        "browsersync:native": "browser-sync start --no-open --log-level silent --reload-debounce 3000 --proxy $(grep DOMAIN .env | cut -d '=' -f2) --port 3030 --files \"./$npm_package_config_build_folder/*.html, ./$npm_package_config_build_folder/*.css, ./$npm_package_config_build_folder/*.js\"",
        "                                                                                                                                                                                                         ": "",
