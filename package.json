        "delete:files": "echo \"â³\" && del-cli ./$npm_package_config_build_folder/*.* ./$npm_package_config_build_folder/**/ !./$npm_package_config_build_folder/index.html !./$npm_package_config_build_folder/favicon.png !./$npm_package_config_build_folder/_fonts/ && echo \"âœ…\"",
